{
	"name": "obsidian-hugo-export",
	"version": "0.1.0",
	"description": "Export journal entries from Obsidian to Hugo",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin",
		"hugo"
	],
	"author": "samf",
	"license": "MIT",
	"devDependencies": {
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^20.10.0",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "^0.19.10",
		"obsidian": "latest",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"js-yaml": "^4.1.1"
	}
}
